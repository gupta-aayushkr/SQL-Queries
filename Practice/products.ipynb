        {
            "cell_type": "code",
            "source": [
                "SELECT p.productName, SUM(o.quantity * p.price) AS totalRevenue\n",
                "FROM Products p\n",
                "JOIN Orders o ON p.productID = o.productID\n",
                "GROUP BY p.productName\n",
                "ORDER BY totalRevenue DESC;"
            ],
